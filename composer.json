{
    "name": "masteryuri/laravel-pub-preview",
    "type": "library",
    "description": "Get resized image from public directory",
    "keywords": [
        "masteryuri",
        "laravel-pub-preview",
        "scale image",
        "resize image",
        "image fitin fitout",
        "resize on fly",
        "scale on fly",
        "generate preview on fly"
    ],
    "homepage": "https://github.com/MasterYuri/Laravel-PubPreview",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuriy Guzenko",
            "email": "yuriyg86@gmail.com",
            "homepage": "https://github.com/MasterYuri",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "MasterYuri\\PubPreview\\": "src"
        },
        "files": [
            "src/funcs.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MasterYuri\\PubPreview\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
